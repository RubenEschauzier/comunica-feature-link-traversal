{
  "name": "@comunica/statistic-write-to-file-overwrite",
  "version": "0.6.0",
  "description": "Comunica package that defines a package for writing statistic output to file that is overwritten in each update.",
  "license": "MIT",
  "homepage": "https://comunica.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/comunica.git",
    "directory": "packages/statistic-link-dereference"
  },
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "keywords": [
    "comunica",
    "statistic",
    "link dereference"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "typings": "lib/index",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "bin/**/*.js.map",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.js.map"
  ],
  "scripts": {
    "build": "node \"../../node_modules/typescript/bin/tsc\""
  },
  "dependencies": {
    "@comunica/core": "^4.0.1",
    "@comunica/statistic-base": "^4.0.1",
    "@comunica/types": "^4.0.1"
  }
}
